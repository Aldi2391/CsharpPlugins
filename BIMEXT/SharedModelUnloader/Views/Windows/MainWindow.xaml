<Window x:Class="SharedModelUnloader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SharedModelUnloader"
        mc:Ignorable="d"
        Title="Выберите команду" Height="250" Width="300"
        ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        >
    <StackPanel Orientation="Vertical" Margin="10">
        <Button Content="Переход на рабочие модели"
                Height="40"
                />
        <Button Content="Выдача изменений"
                Height="40"
                Margin="0 10 0 0"
                Command="{Binding OutputChangesCommand}"
                CommandParameter="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type Window}}}"/>
        <Button Content="Принятие изменений"
                Height="40"
                Margin="0 10 0 0"
                />
        <Button Content="Истории обмена"
                Height="40"
                Margin="0 10 0 0"
                />
    </StackPanel>
</Window>
